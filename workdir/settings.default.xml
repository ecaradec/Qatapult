<?xml version="1.0"?>
<settings>
	<general>
		<font>Arial</font>
		<skin>inline</skin>
	</general>
	<gmailContacts>
		<accessToken></accessToken>
		<refreshToken></refreshToken>
		<email />
		<username />
		<password />
		<server></server>
		<updated-min></updated-min>
	</gmailContacts>
	<hotKeys>
		<toggleKey>32</toggleKey>
		<toggleModifier>4</toggleModifier>
	</hotKeys>
	<sources>
		<IndexedFiles>
			<default>1</default>
			<enabled>1</enabled>
		</IndexedFiles>
		<Filesystem>
			<default>1</default>
			<enabled>1</enabled>
		</Filesystem>
		<Network>
			<default>1</default>
			<enabled>1</enabled>
		</Network>
		<Contacts>
			<default>1</default>
			<enabled>1</enabled>
		</Contacts>
		<Websites>
			<default>1</default>
			<enabled>1</enabled>
		</Websites>
		<FileHistory>
			<default>1</default>
			<enabled>1</enabled>
		</FileHistory>
		<ExplorerSelection>
			<default>1</default>
			<enabled>1</enabled>
		</ExplorerSelection>
		<Windows>
			<default>0</default>
			<enabled>1</enabled>
		</Windows>
	</sources>
	<rules>
		<rule>
			<arg>FILE</arg>
			<arg>
				<item>
					<lbl>Explore here</lbl>
					<ico>icons\defaultverb.png</ico>
				</item>
			</arg>
			<cmd>explorer.exe</cmd>
			<args>/e,/root,"$0.rdirectory"</args>
		</rule>
		<rule>
			<arg>FILE</arg>
			<arg>
				<item>
					<lbl>Cmd here</lbl>
					<ico>icons\command.png</ico>
				</item>
			</arg>
			<cmd>cmd.exe</cmd>
			<workdir>$0.rdirectory</workdir>
		</rule>
 </rules>
	<searchFolders>
		<folder></folder>
	</searchFolders>
</settings>
